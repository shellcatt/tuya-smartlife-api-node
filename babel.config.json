{
	"presets": [["@babel/preset-env", { "modules": "auto" }]],
	"plugins": [
		["@babel/plugin-transform-modules-commonjs"],
		["babel-plugin-transform-import-meta"],
	],
	"env": {
	  "build-esm": {
		"presets": [["@babel/preset-env", { "modules": false }]]
	  },
	  "build-cjs": {
		"presets": [["@babel/preset-env", { "modules": "commonjs" }]]
	  }
	}
}